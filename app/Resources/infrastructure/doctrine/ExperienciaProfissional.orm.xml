<?xml version="1.0" encoding="utf-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity repository-class="Infrastructure\Persistence\Doctrine\ExperienciaProfissionalRepository"
            name="Domain\Model\ExperienciaProfissional"
            table="ExperienciaProfissional">
        <id name="idExperienciaProfissional" type="integer">
            <generator strategy="AUTO"/>
        </id>

        <field name="cargo" type="string" column="cargo" />
        <field name="descricao" type="string" column="descricao" />
        <field name="periodoInicial" type="datetime" column="periodoInicial" />
        <field name="periodoFinal" type="datetime" column="periodoFinal" />
        <field name="trabalhoAtual" type="boolean" column="trabalhoAtual" />

    </entity>
</doctrine-mapping>